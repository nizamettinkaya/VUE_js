AA,KAAA,EAAA,CAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OATA;AAUA;AAxBA,GAVA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,wDAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,WAAA,IAAA,GAAA,IAAA,QAAA,CAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,YAAA,IAAA,GAAA;AACA,UAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA,GAAA,EAAA,IADA;AAEA,UAAA,EAAA,EAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,KAXA,EAYA,KAZA;AAaA;AAlDA,CAAA","sourcesContent":["<template>\n  <div id=\"container\">\n    <div class=\"row\">\n      <div class=\"col-md-8 offset-md-2 text-center\">\n        <h3 class=\"mt-5\">Todo List | Vue.js</h3>\n        <hr>\n        <div class=\"row\">\n          <div class=\"col-md-6 offset-md-3 d-flex flex-row justify-content-between align-items-center\">\n            <input type=\"text\" v-model=\"todoText\"> \n            <button @click=\"addTodo()\" class=\"btn btn-primary\">Ekle</button>\n          </div>\n        </div>\n        <hr>\n        <div class=\"todo-container\">\n          <Todo @deleteTodo=\"deleteTodo($event)\" v-for=\"todo in todoList\" :todo=\"todo\" :key=\"todo.id\" />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Todo from \"@/components/Todo\"\nimport axios from \"axios\"\nexport default {\n  components : {\n    Todo\n  },\n  data(){\n    return {\n      todoText : \"\",\n      todoList : []\n    }\n  },\n  methods : {\n    addTodo(){\n      axios.post(\"https://vue-first-project-57401-default-rtdb.firebaseio.com/todoList.json\", { text : this.todoText})\n      .then(response => {\n        this.todoList.push({\n          id : response.data.name,\n          text: this.todoText\n        })\n      })\n      .catch(e => {\n        console.log(e)\n      })\n    },\n    deleteTodo(todoId){\n      axios.delete(\"https://vue-first-project.firebaseio.com/todoList/\" + todoId + \".json\")\n      .then(response => {\n          let index = this.todoList.findIndex(i => {\n            return i.id == todoId\n          })\n          this.todoList.splice(index, 1)\n      })\n      .catch(e => {\n        console.log(e)\n      })\n    }\n  },\n  created(){\n      axios.get(\"https://vue-first-project.firebaseio.com/todoList.json\")\n      .then(response => {\n        console.log(response.data)\n        for(let key in response.data){\n          console.log(response.data[key])\n          let todo = {\n            text : response.data[key].text,\n            id : key\n          }\n          this.todoList.push(todo)\n        }\n      })\n      .catch()\n    }\n}\n</script>"],"sourceRoot":"src"}]}